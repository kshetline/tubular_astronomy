{
  "name": "@tubular/astronomy",
  "version": "3.5.3",
  "description": "Astronomical calculations for planetary positions, moon phases, eclipses, rise, transit, and set times, and more.",
  "module": "dist/index.min.mjs",
  "main": "dist/index.min.cjs",
  "browser": "dist/index.min.js",
  "umd:main": "dist/index.min.js",
  "typings": "dist/index",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.min.mjs",
      "require": "./dist/index.min.cjs",
      "browser": "./dist/index.min.js",
      "default": "./dist/index.min.cjs"
    },
    "./dist/cjs/index.js": "./dist/index.min.cjs",
    "./dist/fesm2015/index.js": "./dist/index.min.mjs",
    "./dist/umd/index.js": "./dist/index.min.js"
  },
  "engines": {
    "node": ">=10.24.1"
  },
  "scripts": {
    "build": "rimraf dist/ && rollup -c && npm run resources",
    "coverage": "nyc report --reporter=text-lcov > coverage/lcov.info",
    "prepack": "npm run lint && npm run test && npm run build",
    "lint": "eslint --ext .ts 'src/**/*.ts'",
    "test": "nyc --reporter=text --reporter=html mocha --require ts-node/register src/**/*.spec.ts && npm run coverage",
    "resources": "cp -R src/resources/ dist/resources/"
  },
  "keywords": [
    "asteroids",
    "astronomy",
    "calendar",
    "comets",
    "eclipses",
    "equinox",
    "grs",
    "moon",
    "planets",
    "solar",
    "solstice",
    "stars",
    "sunrise",
    "sunset"
  ],
  "author": "Kerry Shetline <kerry@shetline.com>",
  "license": "MIT",
  "dependencies": {
    "@tubular/array-buffer-reader": "^3.0.3",
    "@tubular/math": "^3.4.0",
    "@tubular/time": "^3.10.3",
    "@tubular/util": "^4.18.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@types/chai": "^4.3.16",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^22.15.31",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "chai": "^4.4.1",
    "eslint": "^9.29.0",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.43.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "overrides": {
    "@rollup/pluginutils": {
      "rollup": "$rollup"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshetline/tubular_astronomy.git"
  }
}
